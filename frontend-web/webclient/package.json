{
    "name": "ucloud",
    "version": "0.44.8",
    "description": "UCloud",
    "author": "IMADA - eScience Center, SDU",
    "scripts": {
        "start": "webpack serve --config webpack.dev.js --inline --progress --port 9000 --host 0.0.0.0",
        "start_use_local_backend": "npm run start -- --env local_dev",
        "clean": "rm -rf dist/",
        "build": "npm run clean && node scripts/update-version-file.js && webpack --config webpack.prod.js --progress",
        "test": "jest --runInBand",
        "prepare-icons": "npm run icons-clean && npm run icons-components && npm run icons-index",
        "icons-clean": "cd app/ui-components/icons; rm -rf components index.ts",
        "icons-components": "cd app/ui-components/icons; svgr . --ext=tsx --filename-case camel -d components && rm -f components/index.tsx",
        "icons-index": "node scripts/create-icons-index.js"
    },
    "dependencies": {
        "@novnc/novnc": "^1.2.0",
        "@svgr/cli": "^5.5.0",
        "date-fns": "^2.21.2",
        "fast-deep-equal": "^3.1.3",
        "fuse.js": "^6.4.6",
        "localforage": "^1.9.0",
        "path-browserify": "^1.0.1",
        "react": "17.0.2",
        "react-datepicker": "3.8.0",
        "react-dom": "17.0.2",
        "react-dropzone": "^11.3.2",
        "react-markdown": "^5.0.3",
        "react-modal": "^3.13.1",
        "react-redux": "^7.2.4",
        "react-router": "5.2.0",
        "react-router-dom": "5.2.0",
        "react-syntax-highlighter": "^15.4.3",
        "react-transition-group": "^4.4.1",
        "recharts": "^2.0.9",
        "redux": "4.0.5",
        "redux-responsive": "^4.3.8",
        "styled-components": "^5.3.0",
        "styled-system": "^5.1.5",
        "svgi": "^1.1.0",
        "ua-parser-js": "^0.7.28",
        "xterm": "^4.11.0",
        "xterm-addon-fit": "^0.5.0"
    },
    "devDependencies": {
        "@testing-library/dom": "^7.30.4",
        "@testing-library/react": "^11.2.6",
        "@types/history": "^4.7.8",
        "@types/jest": "26.0.23",
        "@types/json-schema": "^7.0.7",
        "@types/react": "17.0.5",
        "@types/react-datepicker": "3.1.8",
        "@types/react-dom": "^17.0.3",
        "@types/react-dropzone": "^5.1.0",
        "@types/react-modal": "3.12.0",
        "@types/react-redux": "^7.1.16",
        "@types/react-router": "^5.1.14",
        "@types/react-router-dom": "^5.1.7",
        "@types/react-test-renderer": "17.0.1",
        "@types/react-transition-group": "^4.4.1",
        "@types/recharts": "^1.8.19",
        "@types/styled-components": "5.1.5",
        "@types/styled-system": "^5.1.11",
        "@types/ua-parser-js": "^0.7.35",
        "@typescript-eslint/eslint-plugin": "^4.22.1",
        "@typescript-eslint/parser": "^4.22.1",
        "compression-webpack-plugin": "^7.1.2",
        "copy-webpack-plugin": "8.1.1",
        "css-loader": "^5.2.4",
        "empty-module": "~0.0.2",
        "eslint": "^7.25.0",
        "eslint-plugin-react": "^7.23.2",
