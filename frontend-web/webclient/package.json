        "ts-loader": "9.1.1",
        "typescript": "^4.2.4",
        "url-loader": "4.1.1",
        "webpack": "5.36.2",
        "webpack-cli": "^4.6.0",
        "webpack-dev-server": "3.11.2",
        "webpack-merge": "5.7.3"
    }
